model EncryptionKey {
  id          String   @id @default(cuid())
  data        String   @default("")  // Store encrypted data as JSON string
  publicKey   String
  privateKey  String
  algorithm   String   @default("AES-256-GCM")
  created     Int
  expires     Int
  usage       String
  active      Boolean  @default(true)
  createdAt   DateTime @default(now())
}
